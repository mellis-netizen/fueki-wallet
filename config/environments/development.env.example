# Development Environment Configuration
# Copy to .env.development and fill in your values

# API Configuration
API_BASE_URL=https://dev-api.fueki.io
API_TIMEOUT=30000
API_RETRY_ATTEMPTS=3

# Blockchain Networks
BITCOIN_NETWORK=testnet
ETHEREUM_NETWORK=goerli
ETHEREUM_RPC_URL=https://goerli.infura.io/v3/YOUR_PROJECT_ID

# TSS Configuration
TSS_THRESHOLD=2
TSS_TOTAL_SHARES=3
TSS_SERVER_URL=https://dev-tss.fueki.io

# Feature Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_ADVANCED_SETTINGS=true
ENABLE_DEBUG_MODE=true
ENABLE_TEST_NETWORKS=true

# Security
SESSION_TIMEOUT=1800000
MAX_LOGIN_ATTEMPTS=5
ENCRYPTION_ALGORITHM=aes-256-gcm

# Analytics
ENABLE_ANALYTICS=false
ANALYTICS_ENDPOINT=

# Logging
LOG_LEVEL=debug
LOG_TO_CONSOLE=true
LOG_TO_FILE=false

# App Configuration
APP_VERSION=1.0.0
MIN_SUPPORTED_VERSION=1.0.0
FORCE_UPDATE=false

# External Services
PRICE_FEED_URL=https://api.coingecko.com/api/v3
EXCHANGE_RATE_API=https://api.exchangerate-api.com/v4/latest

# Push Notifications
FCM_SERVER_KEY=
APNS_KEY_ID=
APNS_TEAM_ID=

# Deep Linking
APP_SCHEME=fueki
UNIVERSAL_LINK_DOMAIN=dev.fueki.io

# Development Tools
ENABLE_REDUX_DEVTOOLS=true
ENABLE_REACTOTRON=true
STORYBOOK_ENABLED=true
